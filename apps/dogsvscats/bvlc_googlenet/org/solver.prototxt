# Based on https://github.com/BVLC/caffe/tree/master/models/bvlc_googlenet
net: "bvlc_googlenet/org/train_val.prototxt"
test_iter: 1000
test_interval: 1000
test_initialization: false
display: 40
average_loss: 40
base_lr: 0.01
lr_policy: "step"
stepsize: 2500
gamma: 0.96
max_iter: 40000
momentum: 0.9
weight_decay: 0.0002
snapshot: 1000
snapshot_prefix: "bvlc_googlenet/org/bvlc_googlenet"
solver_mode: GPU
